    "@tiptap/starter-kit": "^2.12.0",
    "install": "^0.13.0",
    "next": "15.1.8",
    "npm": "^11.4.1",
    "prisma": "^6.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
